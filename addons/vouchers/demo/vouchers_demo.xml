        <!-- Demo Expense Categories -->
        <record id="expense_category_opex" model="kst.expense.category">
            <field name="name">OPEX (Operational Expenses)</field>
        </record>
        <record id="expense_category_capex" model="kst.expense.category">
            <field name="name">CAPEX (Capital Expenses)</field>
        </record>
        <record id="expense_category_administrative" model="kst.expense.category">
            <field name="name">Administrative</field>
        </record>

        <!-- Demo Expenses -->
        <record id="expense_electricity" model="kst.expense">
            <field name="category_id" ref="expense_category_opex"/>
            <field name="name">Electricity Bill</field>
            <field name="expense_type">Utilities</field>
            <field name="general_group">Operations</field>
        </record>
        <record id="expense_water" model="kst.expense">
            <field name="category_id" ref="expense_category_opex"/>
            <field name="name">Water Bill</field>
            <field name="expense_type">Utilities</field>
            <field name="general_group">Operations</field>
        </record>
        <record id="expense_rent" model="kst.expense">
            <field name="category_id" ref="expense_category_opex"/>
            <field name="name">Office Rent</field>
            <field name="expense_type">Rent</field>
            <field name="general_group">Operations</field>
        </record>
        <record id="expense_maintenance" model="kst.expense">
            <field name="category_id" ref="expense_category_opex"/>
